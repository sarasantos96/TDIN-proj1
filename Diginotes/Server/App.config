<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  </configSections>
  <system.runtime.remoting>
    <application name="Server">
      <service>
        <wellknown mode="Singleton" type="Registry.Registry, Registry"
                   objectUri="Registry" />
      </service>
      <channels>
        <channel ref="tcp" port="9000">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>